block breadcrumbs
-var list = { 'Главная': 'index.html', 'Каталог': 'catalog.html', 'iPad Air Wi-Fi + Cellular 1 28GB - Silver': '/'};
.breadcrumbs-block
	ul.breadcrumbs-list
		each val, key in list
			if selected === key
				li.breadcrumbs-item.breadcrumbs-item-active
					a.breadcrumbs-item-link(href=val)= key
			else
				li.breadcrumbs-item
					a.breadcrumbs-item-link(href=val)= key